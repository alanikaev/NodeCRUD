<%- include layouts/header.ejs %>

        <div class="page-data">
            
            <form method="post" action="/customers/add">
                <table cellpadding="11">
                <tr>
                    <td class="label">Имя</td><td>: <input type="text" name="name" required></td>
                 </tr>
                 <tr>
                    <td class="label" valign="top">Фамилия</td><td valign="top">: <input type="text" name="family" required</td>
                </tr>
                <tr>
                    <td class="label">Логин</td><td>: <input type="text" name="login" required></td>
                 </tr>
                  <tr>
                    <td class="label">Пароль</td><td>: <input type="password" name="password" required></td>
                 </tr>
                  <tr>
                    <td class="label">Город</td><td>: <input type="text" name="city" required></td>
                 </tr>
                 <tr>
                    <td class="label"></td>
                    <td>
                        &nbsp; <input class="btn" type="submit" name="submit" value="Сохранить">
                       <input type="button" name="cancel" value="Отменить" class="cancel btn" onClick="cancelAdd()">
                    </td>
                 </tr>
                </table>
            </form>
            
        </div>
        
<%- include layouts/footer.ejs %>
